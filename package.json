{
  "name": "product-feedback-app-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "dev": "concurrently -n NEXT,TS -c magenta,cyan \"next dev\" \"yarn ts --watch\"",
    "build": "next build",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "start": "next start",
    "db:push": "prisma db push",
    "test": "jest --maxWorkers=50% --passWithNoTests -u",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --ci --passWithNoTests --runInBand -u"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@hookform/resolvers": "^2.9.10",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@next/font": "^13.0.1",
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.10.0",
    "@trpc/client": "10.0.0-rc.0",
    "@trpc/next": "10.0.0-rc.0",
    "@trpc/react-query": "10.0.0-rc.0",
    "@trpc/server": "10.0.0-rc.0",
    "class-variance-authority": "^0.2.4",
    "lucide-react": "^0.98.0",
    "next": "13.0.0",
    "next-auth": "^4.15.0",
    "react": "18.2.0",
    "react-aria": "^3.20.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.1",
    "superjson": "1.9.1",
    "yup": "^0.32.11",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.5.0",
    "eslint": "8.22.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": ">=12.3.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.5.0",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.6.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "yarn format"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  }
}
